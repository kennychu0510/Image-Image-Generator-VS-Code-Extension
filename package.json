{
  "name": "image-import-generator",
  "displayName": "Image Import Generator",
  "description": "Generates image import boilerplate code in one click",
  "repository": {
    "type": "git",
    "url": "https://github.com/kennychu0510/Image-Image-Generator-VS-Code-Extension"
  },
  "icon": "images/icon.png",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "image-import-generator.generateImportOnce",
        "title": "Image Import Generator: Generate Import File"
      },
      {
        "command": "image-import-generator.generateImportAutoTrack",
        "title": "Image Import Generator: Track Folder"
      },
      {
        "command": "image-import-generator.untrackAll",
        "title": "Image Import Generator: Untrack All Folders"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "submenu": "image-import-generator.submenu",
          "group": "image-import-generator",
          "when": "explorerResourceIsFolder"
        }
      ],
      "image-import-generator.submenu": [
        {
          "command": "image-import-generator.generateImportOnce"
        },
        {
          "command": "image-import-generator.generateImportAutoTrack"
        }
      ]
    },
    "submenus": [
      {
        "id": "image-import-generator.submenu",
        "label": "Image Import Generator"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "concurrently \"webpack --watch\" \"npm run watch-tests\" ",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "unit-tests": "npx vitest"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.82.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "concurrently": "^8.2.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "vitest": "^0.34.4",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "license": "Apache-2.0",
  "publisher": "KennyChu",
  "dependencies": {
    "@types/sinon": "^17.0.3",
    "D": "^1.0.0",
    "sinon": "^17.0.1"
  }
}
